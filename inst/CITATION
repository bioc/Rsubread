citHeader("Please cite Rsubread as below:")

citEntry(entry="Article",
    title = "The Subread aligner: fast, accurate and scalable read mapping by seed-and-vote",
    author = personList(as.person("Liao Y"),as.person("Smyth GK"),as.person("Shi W")),
    journal="Nucleic Acids Research",
    year = 2013,
    volume = 41,
    note = "In Press, accepted on 8 March 2013",

    textVersion = 
    paste("Liao Y, Smyth GK and Shi W (2013). The Subread aligner: fast, accurate and scalable read mapping by seed-and-vote. Nucleic Acids Research, Volume 41, accepted on 8 March")
    )

